<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jhyejin99.inuclub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jhyejin99.inuclub.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jhyejin99.inuclub.Settings.Loading"></activity>
        <activity android:name="com.jhyejin99.inuclub.Settings.Setting_Alarm"></activity>
        <activity android:name="com.jhyejin99.inuclub.Settings.Setting_Club_Edit"></activity>
        <activity android:name="com.jhyejin99.inuclub.Settings.Setting_Event"></activity>
        <activity android:name="com.jhyejin99.inuclub.Settings.Setting_Event_Add"></activity>
        <activity android:name="com.jhyejin99.inuclub.Settings.Login"></activity>
    </application>

</manifest>